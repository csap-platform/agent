				},
				"getVmStatsMeanMs": {
					"simonMedianTime": "java.services.OsManager.getVmStats()",
					"divideBy": "1000000",
					"max": "50",
					"title": "Host Metrics Time (ms)"
				},
				"getVmStatsMaxTimeMs": {
					"simonMaxTime": "java.services.OsManager.getVmStats()",
					"divideBy": "1000000",
					"max": "220",
					"title": "Host Metrics Max(ms)"
				}
			},
