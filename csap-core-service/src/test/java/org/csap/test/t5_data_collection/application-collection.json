				},
				"AdminPingsMeanMs": {
					"simonMedianTime": "http.CsAgent.os.getManagerJson.GET",
					"divideBy": "1000000",
					"max": 25,
					"title": "Manager Ping (ms)"
				},
				"AdminPingsMaxTimeMs": {
					"simonMaxTime": "http.CsAgent.os.getManagerJson.GET",
					"divideBy": "1000000",
					"max": 50,
					"title": "Manager Ping Max(ms)"
				},
				"VmStats": {
